# -------------------------
# Symfony / PHP
# -------------------------
# Environment files (do NOT commit secrets)
# Ignore all env variants that may contain secrets but keep example/dist files
/.env*
!/.env.dist
!/.env.example
/.env.local.php

# Backend environment files
/back/.env*
!back/.env.dist
!back/.env.example

# Symfony secrets and local caches/logs
/config/secrets/
/var/
/**/var/
/back/var/

# Composer dependencies (don't commit vendors)
/vendor/
/**/vendor/
/back/vendor/

# PHPUnit / test artifacts
/phpunit.xml
/.phpunit.cache/

# -------------------------
# Frontend / Node
# -------------------------
# Node modules and package manager logs
/node_modules/
/front/node_modules/
npm-debug.log
pnpm-debug.log
yarn-debug.log
yarn-error.log

# Webpack / Encore / build output
/public/build/
/back/public/build/
/front/dist/
/public/assets/
/assets/vendor/

# Misc build artifacts
/build/
/dist/

# -------------------------
# Logs, caches, coverage
# -------------------------
*.log
/coverage/
/.cache/
/.sass-cache/

